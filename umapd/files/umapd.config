config backhaul 'easymesh'
	option ssid 'easymesh-backhaul'
	option hidden '1'
	option band '5g'
	option authentication 'psk2'
	option key 'secretbackhaulkey'
	list ciphers 'ccmp'

config fronthaul 'home_net_5g'
