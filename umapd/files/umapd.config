	option key 'supersecure123'
	list ciphers 'ccmp'

config fronthaul 'home_net_5g_legacy'
	option ssid 'home-net-5g-legacy'
	option band '5g'
	option authentication 'psk2'
	option key 'lessecure456'
	list ciphers 'ccmp'

config fronthaul 'home_net'
	option ssid 'home-net'
	option band '2g'
	option authentication 'psk2'
